<%- model_class = Prodentrada -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:entrada_id) %>:</strong></dt>
  <dd><%= @prodentrada.entrada_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:produto_id) %>:</strong></dt>
  <dd><%= @prodentrada.produto_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quantidade) %>:</strong></dt>
  <dd><%= @prodentrada.quantidade %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              prodentradas_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_prodentrada_path(@prodentrada), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              prodentrada_path(@prodentrada),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
