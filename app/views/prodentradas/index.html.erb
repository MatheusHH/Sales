<%- model_class = Prodentrada -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:entrada_id) %></th>
      <th><%= model_class.human_attribute_name(:produto_id) %></th>
      <th><%= model_class.human_attribute_name(:quantidade) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @prodentradas.each do |prodentrada| %>
      <tr>
        <td><%= link_to prodentrada.id, prodentrada_path(prodentrada) %></td>
        <td><%= prodentrada.entrada.nf %></td>
        <td><%= prodentrada.produto.nome %></td>
        <td><%= prodentrada.quantidade %></td>
        <td><%=l prodentrada.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_prodentrada_path(prodentrada), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      prodentrada_path(prodentrada),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_prodentrada_path,
            :class => 'btn btn-primary', :style => "display: none" %>
<br>
<br>
<%= paginate @prodentradas %>
